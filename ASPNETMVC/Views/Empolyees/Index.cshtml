@model List<ASPNETMVC.Models.Domain.Employee>
@{

}
<h1>Employee</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>DateofBirth</th>
            <th>Department</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DateOfTime</td>
                <td>@employee.Department</td>
                <td><a href="Empolyees/View/@employee.Id">Views</a></td>
            </tr>
        }
    </tbody>
</table>